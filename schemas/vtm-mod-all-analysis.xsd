 <xs:annotation>
  <xs:documentation xml:lang='en-US'>
    Generic analysis elements, suitable for either
    visual, audio, or print media.

  </xs:documentation>
 </xs:annotation>


 <xs:complexType name='ExtraType'>
  <xs:annotation>
   <xs:documentation xml:lang='en-US'>
    Free-form XML content.
   </xs:documentation>
  </xs:annotation>
  <xs:sequence>
   <xs:any minOccurs='1' maxOccurs='unbounded' processContents='lax'/>
  </xs:sequence>
  <xs:attribute name='name' type='xs:token' use='optional'/>
 </xs:complexType>

 <xs:complexType name='AnalysisType'>
  <xs:annotation>
   <xs:documentation xml:lang='en-US'>
    Analytical information about the given content.
    Content that does not originate from the development
    of the content, but how it's interpreted.
   </xs:documentation>
  </xs:annotation>
  <xs:sequence>
   <xs:element name='extra' minOccurs='1' maxOccurs='unbounded'/>
  </xs:sequence>
 </xs:complexType>

</xs:schema>
